Frantzs-Mac-Pro:~ frantz$ MINOR=0 # int value
Frantzs-Mac-Pro:~ frantz$ PATCH=2 # int value
Frantzs-Mac-Pro:~ frantz$ VERS="$MAJOR.$MINOR.$PATCH"
Frantzs-Mac-Pro:~ frantz$ MARK_VERS="sed -i .bak '2 s/version=\"1.0.0\"/version=\"${VERS}\"/g;' config.xml && rm config.xml.bak &&"
Frantzs-Mac-Pro:~ frantz$ ADD_ANDROID_PLATFORM="if [ ! -d  platforms/android ]; then cordova -d platform add android; fi;"
